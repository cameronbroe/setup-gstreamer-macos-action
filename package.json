{
  "name": "setup-gstreamer-macos-action",
  "version": "0.1.0",
  "private": true,
  "description": "A simple GitHub action to set up GStreamer runtime and development packages for macOS runners",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "prepare-for-commit": "npm run format && npm run build && npm run pack",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cameronbroe/setup-gstreamer-macos-action.git"
  },
  "keywords": [
    "actions",
    "gstreamer",
    "setup"
  ],
  "author": "Cameron Roe",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^0.2.1",
    "@actions/core": "^1.2.6",
    "@actions/tool-cache": "^1.5.5",
    "lodash": "^4.17.15",
    "superagent": "^5.3.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.157",
    "@types/node": "^12.7.12",
    "@types/superagent": "^4.1.8",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
